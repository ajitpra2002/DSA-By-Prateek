	public static Integer trapWater(int arr[]) {
//		arr = [0,1,0,2,1,0,1,3,2,1,2,1]
		for(int i=0;i<arr.length;i++)
		{ 
			int NotTrap=arr.length-arr[i]*arr.length;
			if(NotTrap==arr.length) {
				continue;
			}
			else if(NotTrap!=arr.length) {
				trapCount=arr.length-
			}
		}
	}

}
