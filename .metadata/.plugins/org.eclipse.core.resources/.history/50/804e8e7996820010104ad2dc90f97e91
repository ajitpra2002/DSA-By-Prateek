package V1;

public class Mountain06 {
	public static void main(String[] args) {
		
	}
	public static void findlargestMountain(int arr[]) {
		int incresing=0;
		int decresing =0;
		int distanceSum=0;
		for(int i=0;i<arr.length;i++) {
			for(int j=i+1;j<arr.length;j++) {
				if(arr[j]>arr[i]) {
					incresing++;
				}
				
			}
		}
		
	}

}
